<div class="row">
    <h2 class="header white-text">Iniciar Sesión</h2>
    <form class=" card col s12 grey darken-3" style="margin-top: 2%;">
        <div class="row">
            <div class="input-field col s12">
                <input id="id" type="text" class="validate white-text">
                <label for="id">Cédula</label>
            </div>
        </div>
        <div class="row">
            <div class="input-field col s12">
                <input id="password" type="password" class="validate white-text">
                <label for="password">Password</label>
            </div>
        </div>
        <div class="row center">
            <button class="btn waves-effect waves-light" type="submit" name="action">
                Iniciar Sesión
                <i class="material-icons right">send</i>
            </button>
        </div>
    </form>
</div>