                            <tr>
                                <td><i class="medium material-icons white-text">rowing</i>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;Tipo Rutina:</td>
                            </tr>
                            <tr>
                                <td></td>
                                <td align="right">x</td>
                            </tr>
                            <tr>
                                <td><i class="medium material-icons white-text">directions_run</i>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;Descripción de la rutina:</td>
                            </tr>
                            <tr>
                                <td>x</td>
                            </tr>
                        </table>


                    </div>
                </div>
            </div>
        </div>
</div>
    }
    @if (Session["UserType"] == null | (Session["UserType"] != null && !(Session["UserType"].Equals("1") | Session["UserType"].Equals("2"))))
    {
        Response.Redirect("~/Information/_Denied");
    }
